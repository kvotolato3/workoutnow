<div class="container">
  <div class="row">
    <div class="col-md-3"></div>
    <div class="col-md-6">
      <h3>New Exercise</h3>
      <%= form_for @exercise, url: {action: "create"}, class: "form", role: "form" do |f| %>
      <%= f.text_field :name %>
      <%= f.number_field :repetitions %>
      <%= f.submit "Create" %>
      <% end %>
    </div>
    <div class="col-md-3"></div>
  </div><!--  /.row -->
</div> <!-- /.container -->
