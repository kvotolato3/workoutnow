
  <% exercises.each do |exercise| %>
  <div class="panel panel-default clearfix">
  <div class="panel-heading">
  <% if @edit == true %>
    <div class="text-right">
    <i class="fa fa-times" id="wo-ex-remove" data-id="<%= exercise.id %>"></i>
    </div>
  <% end %>
    <h3><%= exercise.name %></h3>
  </div>
    <div class="wo-exercise-img-container">
    <%= image_tag exercise.photo_url, class: "wo-exercise-img" %>
    </div>
    <ul>
    <li><%= exercise.description %></li>
    <li>Repetitions: <%= exercise.repetitions %></li>
    <li>Duration: <%= exercise.duration %></li>
    <li>Source: <%= exercise.source_url %></li>
  </div><!-- /.panel-default -->
  <% end %>



