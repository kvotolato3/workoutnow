
  <% entries.each do |entry| %>
  <div class="panel panel-default clearfix" id="wo-exercise-<%=entry[:workout_exercise_id]%>">
  <div class="panel-heading">
  <% if @edit == true %>
    <div class="text-right">
    <button class="wo-ex-remove fa fa-times btn btn-danger btn-xs" data-id="<%= entry[:workout_exercise_id] %>"> Delete</button>
    </div>
  <% end %>
    <h3><%= entry[:exercise].name %></h3>
  </div>
    <div class="wo-exercise-img-container">
    <%= image_tag entry[:exercise].photo_url, class: "wo-exercise-img" %>
    </div>
    <ul>
    <li><%= entry[:exercise].description %></li>
    <li>Repetitions: <%= entry[:exercise].repetitions %></li>
    <li>Duration: <%= entry[:exercise].duration %></li>
    <li>Source: <%= entry[:exercise].source_url %></li>
  </div><!-- /.panel-default -->
  <% end %>



