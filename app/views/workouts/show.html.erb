
<div class="container">
  <div class="col-md-2"></div>
  <div class="col-md-8">
    <div class="panel panel-default">
    <div class="panel panel-header">
      <div class="text-right">
      <%= link_to '', edit_workout_path(@workout), class: "fa fa-lock", id: "wo-locked" %>
      <%= link_to '', @workout, method: :delete, class: "fa fa-times text-right", id: "wo-remove",
        data: { confirm: 'Are you sure you want to delete this workout?' } %>
      </div>
      <h1><%= @workout.name %></h1>
    </div>
      <%= render 'displayworkout', exercises: @exercises %>
    </div>
  </div>
  <div class="col-md-2"></div>
</div>
